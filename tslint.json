{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "quotemark": [true, "single", "jsx-single"],
    "no-shadowed-variable": false
  },
  "lib": ["es2017"]
}
